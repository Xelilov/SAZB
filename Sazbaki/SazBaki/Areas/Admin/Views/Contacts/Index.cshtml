@model IEnumerable<SazBaki.Models.Contact>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table">
    <tr>
        <th>
            <h4>Contact Phone</h4>
        </th>
        <th>
            <h4>Contact E-mail</h4>
        </th>
        <th>
            <h4>Contact Adress</h4>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.contact_phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.contact_email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.contact_adres)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }, new { @class = "btn btn-success" })
        </td>
    </tr>
}

</table>
